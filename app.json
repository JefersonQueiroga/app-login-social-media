{
  "expo": {
    "name": "app-login-social-media",
    "slug": "app-login-social-media",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "apploginsocialmedia",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jeferson.apploginsocialmedia"
    },
    "android": {
      "package": "com.jeferson.apploginsocialmedia",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosClientId": "331594167804-5ffcphusqkjj545l8o067e4m4ev0apmu.apps.googleusercontent.com",
          "iosUrlScheme":  "com.googleusercontent.apps.331594167804-5ffcphusqkjj545l8o067e4m4ev0apmu",
          "iosReverseClientId": "com.googleusercontent.apps.331594167804-5ffcphusqkjj545l8o067e4m4ev0apmu"

        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "29e863f9-beb9-4ebd-b9b8-e7c7dd15b7db"
      }
    }
  }
}
